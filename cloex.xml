  <!-- Build and Core -->
  <project path="art" name="AOSPA/android_art" remote="aospa" />
  <project path="bionic" name="AOSPA/android_bionic" remote="aospa" />
  <project path="build/make" name="android_build" remote="cloex" >
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="android_build_soong" remote="cloex" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Device -->
  <project path="device/qcom/common" name="android_device_qcom_common" remote="cloex" >
    <linkfile dest="vendor/qcom/build/tasks/generate_extra_images.mk" src="generate_extra_images.mk"/>
  </project>

  <!-- Frameworks -->
  <project path="frameworks/base" name="android_frameworks_base" remote="cloex" />

  <!-- Packages (CLO Extended) -->
  <project path="packages/apps/Contacts" name="yaap/packages_apps_Contacts" remote="github" revision="thirteen" />
  <project path="packages/apps/Dialer" name="yaap/packages_apps_Dialer" remote="github" revision="thirteen" />
  <project path="packages/apps/DeskClock" name="yaap/packages_apps_DeskClock" remote="github" revision="thirteen" />
  <project path="packages/apps/DocumentsUI" name="LineageOS/android_packages_apps_DocumentsUI" remote="github" revision="lineage-20.0" />
  <project path="packages/apps/ExactCalculator" name="yaap/packages_apps_ExactCalculator" remote="github" revision="thirteen" />
  <project path="packages/apps/Messaging" name="yaap/packages_apps_Messaging" remote="github" revision="thirteen" />
  <project path="packages/apps/SettingsIntelligence" name="LineageOS/android_packages_apps_SettingsIntelligence" remote="github" revision="lineage-20.0" />
  <project path="packages/inputmethods/LatinIME" name="yaap/packages_inputmethods_LatinIME" remote="github" revision="thirteen" />
  <project path="packages/apps/GrapheneCam" name="android_packages_apps_GrapheneCam" remote="cloex" clone-depth="1" />
  <project path="packages/apps/NewPrebuilts" name="android_packages_apps_NewPrebuilts" remote="cloex" clone-depth="1" />

  <!-- Packages -->
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="cloex" />
  <project path="packages/modules/Connectivity" name="android_packages_modules_Connectivity" remote="cloex" />

  <!-- System -->
  <project path="system/core" name="android_system_core" remote="cloex" />

  <!-- Vendor (AOSPA) -->
  <project path="vendor/aospa" name="android_vendor_aospa" remote="cloex" >
    <linkfile src="build.sh" dest="rom-build.sh" />
  </project>

</manifest>
